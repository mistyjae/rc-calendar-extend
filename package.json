{
  "name": "rc-calendar-extend",
  "version": "0.1.0",
  "description": "日期扩展组件，包括年份选择和季度选择",
  "main": "dist/index.js",
  "scripts": {
    "clear": "rimraf node_modules",
    "build": "rimraf dist/* ./index.html && webpack -p --config ./webpack.production.config.js --progress --profile --colors",
    "watch_build": "rimraf dist/* ./index.html && webpack -p --watch --config ./webpack.production.config.js --progress --profile --colors",
    "lang_combine": "babel-node ./script/lang/combineJson.js",
    "lang_gen_xlsx": "babel-node ./script/lang/generateLangXlsx.js",
    "translate": "babel-node ./script/lang/translate.js cp",
    "gen_combine": "babel-node ./script/lang/combineJs.js cp",
    "lang_gen_autotranslate_xlsx": "babel-node ./script/lang/autoTranslateRepeation.js",
    "lang_rebuild": "npm run lang_combine && npm run lang_gen_xlsx && npm run lang_gen_autotranslate_xlsx",
    "lang_rebuild_all": "npm run lang_combine && npm run lang_gen_xlsx && npm run lang_gen_autotranslate_xlsx && npm run lang_read_xlsx temp",
    "lang_read_xlsx": "babel-node ./script/lang/readXlsx2Json.js",
    "lint": "eslint  src/components ",
    "install_test": "cd test/peerDependencies && npm install",
    "test": "babel-node ./node_modules/karma/bin/karma start ./karma.conf.js ",
    "test:debug": "babel-node ./node_modules/karma/bin/karma start ./karma.conf.js --no-single-run ",
    "test:mocha": "mocha --compilers js:babel-core/register -r mock-local-storage --recursive"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.2.1",
    "babel-plugin-transform-es3-property-literals": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "css-loader": "^0.18.0",
    "enzyme": "^2.4.1",
    "es6-promise": "^3.0.2",
    "eslint": "^3.11.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "flow-bin": "^0.36.0",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^1.6.1",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^2.3.4",
    "mock-local-storage": "^1.0.2",
    "paths-js": "^0.3.5",
    "postcss-js": "^0.1.3",
    "postcss-loader": "1.0.0",
    "postcss-mixins": "^4.0.1",
    "postcss-modules-values": "^1.1.2",
    "postcss-simple-vars": "^3.0.0",
    "precss": "1.4.0",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-server": "^1.8.2",
    "xlsx": "^0.8.0",
    "json-loader": "^0.5.4"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x.x",
    "react-dom": "0.14.x || 15.x.x",
    "react-addons-update": "0.14.x || 15.x.x"
  },
  "dependencies": {
    "babel-runtime": "^6.2.1",
    "invariant": "^2.2.2",
    "moment": "^2.17.1",
    "object-assign": "^4.0.1",
    "rc-calendar": "^8.4.6"
  }
}
